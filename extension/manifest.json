{
  "manifest_version": 3,
  "name": "extension Utkarsh",
  "version": "1.0",
  "description": "this is learning phase",
  "action": {
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }
  ],
  "background": {
  "service_worker": "background.js"
  },
  "permissions": ["tabs", "storage"],
  "host_permissions": ["https://*/*"]
}
